<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.developer.cloudprint.ui.History">



    <Button
        android:layout_width="100dip"
        android:layout_height="wrap_content"
        android:layout_weight="33"
        android:id="@+id/printButton"
        android:background="@drawable/roundbutton"
        android:backgroundTint="#99bbff"
        android:textColor="#ffffff"
        android:text="Print Page"

        android:layout_marginRight="20dip"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/copiesId"
        android:layout_width="72dp"
        android:layout_height="40dp"
        android:backgroundTint="#ffffff"
        android:textColor="#000000"
        android:background="@drawable/border"
        android:hint="@string/copies"
        android:textAlignment="center"
        android:inputType="number"
        android:layout_marginLeft="25dip"
        android:layout_marginBottom="15dip" />

    <ListView
        android:id="@+id/tweetListH"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/copiesId" />

</RelativeLayout>
